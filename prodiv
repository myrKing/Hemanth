import java.util.Scanner;
public class ProDiv
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int m=a*b;
        int d=a/b;
        int r=a%b;
        System.out.println(m);
        System.out.println(d);
        System.out.println(r);
    }
}
