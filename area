import java.util.Scanner;
public class Area
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int s=sc.nextInt(); 
        int l=sc.nextInt(); 
        int b=sc.nextInt(); 
        float r=sc.nextFloat();
        int a=s*s;
        int d=l*b;
        double c=3.14*r*r;
        System.out.println(a);
         System.out.println(d); 
          System.out.printf("%.2f\n",c);
    }
}
