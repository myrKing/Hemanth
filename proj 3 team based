void student()
{
    System.out.println("Please Fill Your Details :");
    System.out.println("Enter Your Name...");
    name=sc.next();
    System.out.println("Enter Your Mobile Number:");
    num=sc.nextInt();
    System.out.println("Enter the college that you join:");
    clg=sc.next();
    System.out.println("Enter course :");
    cs=sc.next();
    System.out.println("REGISTRATION SUCCESSFULLY COMPLETED......🥳...THANKKYOU FOR JOINING OUR COLLEGE..!!🙏🙏🙏🙏 ");
    System.out.println("******************************************************************");
    System.out.println("------------------------------------------------------------------");
}
void faculty()
{
    System.out.println("Please Fill Your Details :");
    System.out.println("Enter Your Name...");
    name=sc.next();
    System.out.println("Enter Your Mobile Number:");
    num=sc.nextInt();
    System.out.println("Enter the college that you join:");
    clg=sc.next();
    System.out.println("Enter branch :");
    cs=sc.next();
    System.out.println("REGISTRATION SUCCESSFUL..🙏🙏.NOW YOU ARE THE MEMBER OF OUR FAMILY😊");
    System.out.println("*********************************************************************");
    System.out.println("----------------------------------------------------------------------");
}
}
public class Project3
{
    public static void main(String args[])
    {
    College c=new College();
    Scanner sc=new Scanner(System.in);
    System.out.println("**********🏢COLLEGE INFORMATION SYSTEM🏢**********");
    System.out.println("-----------🏫MOTHER THERESA INSTITUTIONS🏫--------------");
     int ch;
    do
    {
        System.out.println(" 1.About \n 2.Fee Details \n 3.Student Registration \n 4.Faculty Registration");
        ch=sc.nextInt();
        switch(ch)
        {
            case 1:
                c.about();
                break;
            case 2:
                c.fees();
                break;
            case 3:
                c.student();
                break;
            case 4:
                c.faculty();
                break;
                
        }
    }while(ch!=5);
    }
    
}
