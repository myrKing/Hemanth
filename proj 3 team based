            System.out.println("------------------------------------------------------------------");
            break;
        case 3:
            System.out.println("1.FInance-Rs.50k \n 2.Marketing-Rs.40k \n 3.CA-Rs.40k \n 4.Human resource-Rs.50k \n 5.Accounts-Rs.50k \n 6.Networking-Rs.40k ");
