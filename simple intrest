import java.util.Scanner;
public class Simple
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        float c=sc.nextFloat();
        float d=a*b*c/100;
        System.out.printf("%.2f\n",d);
    }
}
